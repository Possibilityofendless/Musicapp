# Database
DATABASE_URL="postgresql://user:password@localhost:5432/musicapp"

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
SORA_API_KEY=your_sora_api_key_here

# Server
PORT=3000
NODE_ENV=development

# Redis (for job queue)
REDIS_URL="redis://localhost:6379"

# Storage (S3 or local)
STORAGE_TYPE=local # or "s3"
STORAGE_PATH=/tmp/musicapp_storage

# Sora API
SORA_API_BASE_URL=https://api.openai.com/v1

# Processing
PYTHON_WORKER_POOL_SIZE=2
PROCESSING_TIMEOUT=600000 # 10 minutes in ms
